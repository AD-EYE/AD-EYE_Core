				</Action>
			</Private>
			
			
			<Private object="Man_200_1">
				<Action>
					<Longitudinal>
						<Speed>
							<Dynamics shape="step"/>
							<Target>
								<Absolute value="0"/>
							</Target>
						</Speed>
