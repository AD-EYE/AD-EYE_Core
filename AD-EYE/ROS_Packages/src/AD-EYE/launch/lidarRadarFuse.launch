<!-- -->
<launch>
    <arg name="nodeName" default="lidarRadarFuse"/>
    <arg name="inputTopic1" default="input1"/>
    <arg name="inputTopic2" default="input2"/>
    <arg name="outputTopic" default="output"/>

    <node pkg="adeye"
          type="lidarRadarFuse"
          name="$(arg nodeName)"
          output="screen"
          args="$(arg inputTopic1) $(arg inputTopic2) $(arg outputTopic)"/>

</launch>
